{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "MONGODB_URL": "@mongodb_url",
    "FRONTEND_URL": "@frontend_url",
    "PORT": "3000",
    "NODE_ENV": "production"
  }
}
